<!-- General configurations for RIM -->
<generalConfig>

	<!-- Port number for the RIM server which receives requests from application -->
	<RIMserverPortNumber>33333</RIMserverPortNumber>
	
	<!-- IP address and port of the RM server which takes RIM output commands, depends on RM configuration -->
	<RMserverAddress>ws://localhost:10000</RMserverAddress>

	<!-- The interaction generation scheme, either SINGLE or VECTOR -->
	<generationScheme>VECTOR</generationScheme>
	
</generalConfig>