<!-- All motion parts must be registered in motion_part_list.xml. -->

<robotTypeToMotionPartMapping>

	<robotType name="FuroiHome">
		<motionPart name="LED"></motionPart>
		<motionPart name="speech"></motionPart>
	</robotType>
	
	<robotType name="FuroiParrot">
		<motionPart name="LED"></motionPart>
		<motionPart name="speech"></motionPart>
		<motionPart name="arms"></motionPart>
	</robotType>

</robotTypeToMotionPartMapping>